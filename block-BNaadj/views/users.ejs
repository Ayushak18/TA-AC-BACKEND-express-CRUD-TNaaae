<!DOCTYPE html>
<html lang="en">
  <head>
    <title>Users</title>
  </head>
  <body>
    <h1>All users ejs</h1>
    <% users.forEach((user)=>{ %>
    <h1><a href="/users/<%= user.id %> "><%= user.name %></a></h1>
    <p>Email-<%= user.email %></p>
    <p>Adrress-<%= user.address %></p>
    <p>Bio-<%= user.bio %></p>
    <p><%= user.hobbies %></p>
    <% }) %>
  </body>
</html>
